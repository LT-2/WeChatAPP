/* pages/bottom_menu/botton_menu .wxss */

.bottonrow{
  background-color: white;
  width:100%;
  height: 9%;
  bottom: 0rpx;
  position: fixed;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.bottontab{
  width: 250rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.bottonImg{
  margin-top: 10rpx;
  width: 120rpx;
  height: 100rpx;
  border-radius: 10rpx;
}

.bottontabText{
  font-size: 35rpx;
  color: #979797;
}





